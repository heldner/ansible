# Static resolv.conf file for initial ansible rollup
#     DO NOT EDIT THIS FILE BY HAND -- YOUR CHANGES WILL BE OVERWRITTEN
{% if ansible_local.main.mydomain is defined %}
search {{ ansible_local.main.mydomain }}
{% endif %}
{% for dns_ip in dnsmasq_nameservers %}
{%  if dns_ip | ipv4 or dns_ip | ipv6  %}
nameserver {{dns_ip}}
{%  endif %}
{% endfor %}
