mpd_music_dir = "{{ ansible_user_dir }}/Music"
mpd_host = "{{ mpd_host }}"
mpd_crossfade_time = 10
allow_for_physical_item_deletion = "yes"
song_columns_list_format = "(25)[green]{a} (40)[]{t|f} (30)[red]{b} (7f)[blue]{l}"
playlist_display_mode = "columns"
browser_display_mode = "columns"
visualizer_in_stereo = "yes"
visualizer_type = "spectrum"
user_interface = "alternative"
visualizer_color = "green"
song_library_format = "{% raw %}{%n - }{%t}|{%f}{% endraw %}"
display_bitrate = "yes"
