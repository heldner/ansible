XF86Audio{Lower,Raise}Volume
  pulsemixer --change-volume {-,+}2 --max-volume 100

XF86AudioMute
  pulsemixer --toggle

XF86Audio{Prev,Next,Play}
  mpc -q {prev,next,toggle} > /dev/null

XF86Launch1
  /usr/local/bin/firefox

XF86ScreenSaver
  slock

XF86Display
  xset dpms force off

Print
  flameshot gui

super + a
  action_menu_run

super + s
  ~/.dwm/actions/ssh
