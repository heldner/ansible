{
  "log": {
    "loglevel": "{{ v2ray_loglevel | default('none') }}"
  },
    "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": 1234 ,
      "protocol": "vless",
      "settings": {
        "clients": [
          {% for user in v2ray_users -%}
          {
            "id": "{{ user.id }}"
          }{% if not loop.last %},{% endif %}
          {% endfor %}
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/{{ v2ray_ws_path }}"
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
        "tls"
        ]
      }
    }
  ],
  "outbounds": [
  {
    "tag": "direct",
    "protocol": "freedom",
    "settings": {}
  },
  {
    "tag": "blocked",
    "protocol": "blackhole",
    "settings": {}
  }
  ]
}
